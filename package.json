    "laravel-mix": "^1.0",
    "lightbox2": "^2.10.0",
    "lodash": "^4.17.5",
    "vue": "^2.5.16"
  },
  "dependencies": {
    "dropzone": "^5.5.1",
    "fullcalendar": "^3.9.0",
    "npm": "^5.10.0",
    "select2": "^4.0.6-rc.1",
