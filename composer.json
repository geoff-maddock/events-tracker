{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"fideloper/proxy": "^4.0",
		"laravel/framework": "^7.0",
        "nunomaduro/collision": "^4.1",
		"laravelcollective/html": "^6.0",
		"laravel/socialite": "~4.0",
		"intervention/image": "^2.3",
		"suin/php-rss-writer": "^1.5",
		"laravel/tinker": "^2.0",
		"sentry/sentry": "^2.3",
		"sentry/sentry-laravel": "^1.8",
		"alt-three/bus": "^4.0",
		"laravel-notification-channels/twitter": "^4.0.1",
		"predis/predis": "^1.1",
		"pusher/pusher-php-server": "^4.1",
		"eluceo/ical": "^0.16.0",
        "scottybo/laravel-facebook-sdk": "^5.0",
		"progcode/laravel-fullcalendar": "^1.3",
        "facade/ignition": "^2.0",
        "laravel/ui": "^2.0"
    },
	"require-dev": {
		"fzaninotto/faker": "~1.4",
		"mockery/mockery": "^1.0",
		"phpunit/phpunit": "~8.5",
		"phpspec/phpspec": "~6.0",
		"symfony/dom-crawler": "~3.1",
		"laravel/dusk": "^6.0",
		"filp/whoops": "~2.0",
		"barryvdh/laravel-debugbar":  "^3.5",
		"barryvdh/laravel-ide-helper": "^2.8",
		"nunomaduro/larastan": "^0.6.9"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"App\\": "app/"
		},
		"files": ["app/Http/helpers.php",
					"app/Http/Flash.php"
					]
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		],
		"psr-4": {
			"Tests\\": "tests/"
		},
		"files": ["tests/utilities/functions.php"]
	},
	"scripts": {
		"post-install-cmd": [
			"php-latest artisan clear-compiled"
		],
		"post-update-cmd": [
			"php-latest artisan clear-compiled"
		],
		"post-create-project-cmd": [
			"php-latest -r \"copy('.env.example', '.env');\"",
			"php-latest artisan key:generate"
		],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
